{
  "name": "livepulse",
  "type": "module",
  "description": "LivePulse for AdonisJS",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbsouhail/livepulse.git"
  },
  "homepage": "https://github.com/sbsouhail/livepulse",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./adonisjs/livepulse_provider": "./dist/adonisjs/live_pulse_provider.js",
    "./adonisjs/component": "./dist/adonisjs/component.js"
  },
  "scripts": {
    "build": "tsc && pnpm run build:client",
    "build:client": "vite build",
    "client:watch": "vite build --watch",
